
@{
    ViewBag.Title = "ToDoList";
    Layout = null;
}


<!DOCTYPE html>
<html>
<head>
    <title>Mvc5 ile web geliştirme</title>
    <!-- for-mobile-apps -->
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Profile Widget Responsive, Login form web template, Sign up Web Templates, Flat Web Templates, Login signup Responsive web template, Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <!-- //for-mobile-apps -->
    <link href='//fonts.googleapis.com/css?family=Poppins:400,600,500,700' rel='stylesheet' type='text/css'>
    <link href="~/web/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <script src="~/web/js/jquery-1.11.1.min.js"></script>
    <!-- //js -->
    <!-- Using Jquery --> 
    <script>
    jQuery(document).ready(function($) {
        $('#sidebar-btn').click(function() {
            $('#sidebar').toggleClass('visible');
        });
    });
    </script>
    <!--circlechart-->
    <script src="~/web/js/jquery.circlechart.js"></script>
    <!--circlechart-->

</head>
<body>
    <div class="content">
        <h1>Yapılacaklar Listesi</h1>
        <div class="main">
            <div class="header">
                <div class="hd-img">
                    <img src="~/web/images/resim.jpg" alt=" " class="img-responsive" />
                </div>

                <h2>Yusuf Keleş</h2>
                <p class="photo">Yazılım Mühendisi</p>
                <div class="skill-info">
                    <ul>
                        <li><div class="demo demo-2" data-percent="20"></div><span>Yapılacaklar</span></li>
                        <li><div class="demo demo-2" data-percent="74"></div><span>Tamamlandı</span> </li>
                        <li><div class="demo demo-2" data-percent="30"></div><span>Bekleyen</span> </li>
                    </ul>
                    <script>
                        $('.demo-1').percentcircle({
                            animate: true,
                            fillColor: 'rgb(119, 161, 173)'
                        });
                        $('.demo-2').percentcircle({
                            animate: true,
                            fillColor: 'rgb(0, 197, 255)'
                        });
                        $('.demo-3').percentcircle({
                            animate: true,
                            fillColor: 'rgb(119, 221, 255)'
                        });
                    </script>
                </div>
            </div>
            <section class="slider">
                <div class="flexslider">
                    <ul class="slides">
                        <li>
                            <div class="header-bottom_3">
                                <div class="jan">
                                    <h3>Mart 2023</h3>
                                </div>
                                <div class="jan-bottom">
                                    <div class="jan-bottom-grid">

                                        <div class="jan-bottom-grid-left1">
                                            <p>Finish Home Screen</p>
                                        </div>
                                        <div class="jan-bottom-grid-right">
                                            <p>8 AM</p>
                                        </div>
                                        <div class="clear"></div>
                                    </div>
                                    <div class="jan-bottom-grid">

                                        <div class="jan-bottom-grid-left1">
                                            <p>Lunch Break</p>
                                        </div>
                                        <div class="jan-bottom-grid-right">
                                            <p>12 PM</p>
                                        </div>
                                        <div class="clear"></div>
                                    </div>
                                    <div class="jan-bottom-grid">

                                        <div class="jan-bottom-grid-left1">
                                            <p>Design Meeting</p>
                                        </div>
                                        <div class="jan-bottom-grid-right">
                                            <p>4 PM</p>
                                        </div>
                                        <div class="clear"></div>
                                    </div>
                                    <div class="jan-bottom-grid">

                                        <div class="jan-bottom-grid-left1">
                                            <p>Dinner</p>
                                        </div>
                                        <div class="jan-bottom-grid-right">
                                            <p>8 PM</p>
                                        </div>
                                        <div class="clear"></div>
                                    </div>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="header-bottom_3">
                                <div class="jan">
                                    <h3>Haziran 2023</h3>
                                </div>
                                <div class="jan-bottom">
                                    <div class="jan-bottom-grid">

                                        <div class="jan-bottom-grid-left1">
                                            <p>Finish Home Screen</p>
                                        </div>
                                        <div class="jan-bottom-grid-right">
                                            <p>10 AM</p>
                                        </div>
                                        <div class="clear"></div>
                                    </div>

                                    <div class="jan-bottom-grid">

                                        <div class="jan-bottom-grid-left1">
                                            <p>Lunch Break</p>
                                        </div>
                                        <div class="jan-bottom-grid-right">
                                            <p>1 PM</p>
                                        </div>
                                        <div class="clear"></div>
                                    </div>
                                    <div class="jan-bottom-grid">

                                        <div class="jan-bottom-grid-left1">
                                            <p>Design Meeting</p>
                                        </div>
                                        <div class="jan-bottom-grid-right">
                                            <p>5 PM</p>
                                        </div>
                                        <div class="clear"></div>
                                    </div>
                                    <div class="jan-bottom-grid">

                                        <div class="jan-bottom-grid-left1">
                                            <p>Dinner</p>
                                        </div>
                                        <div class="jan-bottom-grid-right">
                                            <p>9 PM</p>
                                        </div>
                                        <div class="clear"></div>
                                    </div>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </section>
            <!--FlexSlider-->
            <link rel="stylesheet" href="~/web/css/flexslider.css" type="text/css" media="screen" />
            <script defer src="~/web/js/jquery.flexslider.js"></script>
            <script type="text/javascript">
                $(window).load(function () {
                    $('.flexslider').flexslider({
                        animation: "slide",
                        start: function (slider) {
                            $('body').removeClass('loading');
                        }
                    });
                });
            </script>
            <!--End-slider-script-->
        </div>
        <p class="footer">&copy; @DateTime.Now.Year Tüm Hakları Saklıdır | Design by <a href="http://w3layouts.com/"> W3layouts</a></p>
        <a href="/Test/UserList/" class="btn btn-warning">Ana Sayfaya Git</a>
    </div>
    
</body>
</html>
